}
.b5-right .b5-btn{
    width: 120px;
    height: 36px;
    border-radius: 3px;
    color: white;
    text-align: center;
    display: inline-block;
    cursor: pointer;
	position: absolute;
	right: 64%;
	background: url(../images/whiteAdd.png) no-repeat 15%;
