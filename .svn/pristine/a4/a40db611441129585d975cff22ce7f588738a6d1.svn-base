}
.b5-center{
	border: 1px solid #E6E6E6;
	box-shadow: 0 2px 4px 0 rgba(0,0,0,0.20);
	/* padding-bottom: 2%; */
	background-color: #fff;
	height: 70%;
	width: 80%;
	margin: 0 auto;
	/* padding-top: 2%; */
	/* margin-top: 2.4%; */
}
.b5-center ul li{
	position: relative;
	padding: 10px;
	border-bottom: 1px solid #E6E6E6;
}
.b5-center .table-header li>span{
	color: #999999;
	font-family: PingFangSC-Medium;
	font-size: 14px;
    /*padding:1%;*/
    display: inline-block;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}
.b5-center .b5-tbody li>span{
	color: #333333;
	font-family: PingFangSC-Medium;
	font-size: 14px;
	/*padding:1%;*/
	display: inline-block;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}
.b5-tbody li>span:hover{
	text-overflow:inherit;
	overflow:visible;
}
.b5-center .b5-tbody{
	overflow-y: scroll;
	height: 88%;
}
.b5-center>.b5-tbody li{
	padding: 1% 1%;
}
.b5-center>.b5-tbody li>span{
	/*white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;*/
}
.b5-center>.b5-tbody li:nth-child(odd){
	background-color: #fff;
}
.b5-center>.b5-tbody li:nth-child(odd):hover{
	background-color: #F5F5F5;
}
.b5-center>.b5-tbody li:nth-child(even){
	background-color: #fff;
}
.b5-center>.b5-tbody li:nth-child(even):hover{
	background-color:#F5F5F5;
}
.b5-center>ul>li>span:nth-child(2){
	width:16%;
	padding-left: 1%;
}
.b5-center>ul>li>span:nth-child(3){
	width:10%;
}
.b5-center>ul>li>span:nth-child(4){
	width:13.2%;
}
.b5-center>ul>li>span:nth-child(5){
	width:25%;
}
.b5-center>ul>li>span:nth-child(6){
	width:17%;
}
.b5-center>.b5-tbody>li>span:nth-child(2){
	width:16%;
	padding-left: 1%;
}
.b5-center>.b5-tbody>li>span:nth-child(3){
	width:10%;
	padding-left: 1%;
}
.b5-center>.b5-tbody>li>span:nth-child(4){
	width:14%;
	padding-left: 1%;
}
.b5-center>.b5-tbody>li>span:nth-child(5){
	width:25%;
	padding-left: 1%;
}
.b5-center>.b5-tbody>li>span:nth-child(6){
	width:17%;
	padding-left: 1%;
}
.b5-center ul.table-header>li{
	padding: 15px 1%;
	color: #999999;
	background-color: #F9F9F9;
}
.b5-tbodyEidit span{
	color: #469AE0;
	background-color: transparent;
	border: 0;
	font-size:12px;
	margin-left: 4.2%;
