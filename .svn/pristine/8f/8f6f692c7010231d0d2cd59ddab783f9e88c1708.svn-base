</div>
<div id="popLayer"></div>
<!--工程管理-->
<div class="project-manage">
    <div class="container-fluid setWidth">
        <div class="null-div"></div>
        <div class="title-name">所有项目</div>
        <div class="row" id="project-content" style=" overflow-y: scroll;height:560px">
            <div class="col-md-3 col-sm-3 col-xs-4 pl-0 cp mt-20 pubuStyle" id="createNewProject">
                <div class="get-project">
                    <img src="images/add_ico.png">
                    <div class="getNewPro">创建新项目</div>
                </div>
            </div>
        </div>
    </div>
</div>
<script type="text/html" id="project-manage-template">
    {{each}}
    <div class="col-md-3 col-sm-3 col-xs-4 item-template mt-20 pubuStyle" data-id={{$value.pId}}>
        <div class="project one">
            <div class="nunu">
                <img class="enterIndex show_item_img img-responsive center-block" src={{if $value.ppicture ==""}}images/gardenTwo.png{{else}}{{"http://192.168.3.31:8080/bim/getPic?token=" + $value.token + "&loginId=" + $value.loginId + "&fileId=" +$value.ppicture}}{{/if}}>
            </div>
            <div class="project-name">{{$value.pName}}</div>
            <div class="detail">
                {{$value.pIntrouction}}
            </div>
            <div class="row">
                <div class="edit col-md-1 col-sm-4 col-xs-4 col-md-offset-8 col-sm-offset-2 col-xs-offset-2">
                    <img class="project-manageEidit" src="images/edit_ico.png">
                </div>
                <div class="remove col-md-2 col-sm-4 col-xs-6 pr-0">
                    <img src="images/del_ico.png">
                </div>
            </div>
        </div>
    </div>
    {{/each}}
</script>
<!-- 创建新项目的弹出层 -->
<div class="box-cover" id="creatNewItemBoxCover">
    <div class="popups-box" id="creatNewItemPopupsBox">
        <div class="box-close" id="box-close"></div>
        <div class="popups-header">创建项目</div>
        <div class="popups-content">
            <div class="sub-title" id="getfileId">项目封面</div>
            <!-- <div class="popups-img" style="float:left"><img id="defaultImg" src="images/gardenTwo.png"></div> -->
            <div id="uploader-demo">
                <div class="popups-img" style="float:left"><img id="defaultImg" src="images/gardenTwo.png"></div>
                <div id="fileList" class="uploader-list"></div>
                <div id="filePicker">重新选择</div>
            </div>
            <div style="clear:both"></div>
            <div class="sub-title">项目标题</div>
            <div><input type="text" class="sub-input item-title"></div>
            <div class="sub-title">项目描述</div>
            <div>
                <textarea class="sub-textArea item-describe"></textarea>
            </div>
            <div class="footer">
                <div class="btn-ok projectSubmit">提交</div>
            </div>
        </div>
    </div>
</div>
<!-- 手机绑定的弹窗 -->
<div class="alertAccountBindboxcover">
    <div class="alertAccountBindpopupsbox">
        <div class="alertAccountBindboxclose"></div>
        <div class="popups-content">
            <div class="mt-20 tx-c">您的号码为<span id="getTelBindNum"></span>的手机,</div>
            <div class="mt-20 tx-c">会收到一条含有6位数字验证码的短信</div>
            <div class="mt-20 pl-200"><input type="text" placeholder="验证码" class="alertbind fl" id="alertbind">
                <span id="send_verification_phone" class="alertverification fl cp">获取验证码</span>
            </div>
            <div class="cb"></div>
            <div class="alertAccountBindfooter mt-20">
                <div class="alertAccountBindbtnok" id="alertAccountBindbtnok">确定</div>
            </div>
        </div>
    </div>
</div>
<!-- 邮箱绑定弹出框 -->
<div class="emailboxcover">
    <div class="emailpopupsbox">
        <div class="emailboxclose"></div>
        <div class="popups-content">
            <div class="mt-20 tx-c">您的邮箱<span id="getEmailShow"></span>,</div>
            <div class="mt-20 tx-c">会收到一条验证码的信息</div>
            <div class="mt-20 pl-200"><input type="text" placeholder="验证码" class="alertbind fl" id="inputEmailVerification">
                <span id="send_verification_email" class="emailverification fl cp">获取验证码</span>
            </div>
            <div class="cb"></div>
            <div class="eailfooter mt-20">
                <div class="emailbtnok" id="emailbtnok">确定</div>
            </div>
        </div>
    </div>
</div>
<!-- home页面的编辑弹窗 -->
<div class="box-cover" id="editboxcover">
    <div class="popups-box" id="editpopupsbox">
        <div class="box-close" id="editboxclose"></div>
        <div class="popups-header">
            <div class="item-material fl item-edit cp">项目资料</div>
            <div class="item-set fl item-edit cp">项目设置</div>
        </div>
        <div class="cb"></div>
        <div class="editbottom-lines">
            <div class="editbottom-line has-line fl"></div>
            <div class="editbottom-line fl"></div>
        </div>    
        <div class="edit-popups-content">
            <div class="sub-title" id="editItemCover">项目封面</div>
            <div id="uploader-edit">
                <div class="popups-img fl"><img class="editdefaultImg"></div>
                <div id="fileContain" class="uploader-list"></div>
                <div id="aginChoose">重新选择</div>
            </div>
            <div class="cb"></div>
            <div class="sub-title">项目标题</div>
            <div><input type="text" class="sub-input re-item-title"></div>
            <div class="sub-title">项目描述</div>
            <div>
                <textarea class="sub-textArea re-item-describe"></textarea>
            </div>
            <div class="footer">
                <div class="btn-ok re-projectSubmit">提交</div>
            </div>
        </div>
        <div class="second-item-popups-content">
            <div class="item-popups-content">
                <div class="material-configuration">物料配置</div>
                <div class="add-material-sort">
                    <span>添加物料类别</span>
                    <img src="images/add.png">    
                </div>
                <div class="input-material-sort">
                    <textarea placeholder="输入物料类别" class="material-sort-textarea"></textarea>
                    <div class="buttonDiv">
                        <div class="save-edit fr">保存</div>
                        <div class="cancel-edit fr cp">取消</div>                   
                    </div>
                </div>
                <div class="second-input-material-sort">
                    <textarea placeholder="输入物料类别" class="second-material-sort-textarea"></textarea>
                    <div class="second-buttonDiv">
                        <div class="second-save-edit fr">保存</div>
                        <div class="second-cancel-edit fr">取消</div>                   
                    </div>
                </div>
                <div class="appendDiv"></div>
                <div class="mt-20">参与单位</div>
                <div class="detail-participate-unit" id="detail-participate-unit">
                    <img src="images/add.png">
                </div>
                <div class="unRealicationCompany" id="unRealicationCompany">
                    <div class="fr">
                        <div class="fl cancel-firm" id="cancel-unRealicationCompany">取消</div>
                        <div class="fl add-firm" id="add-firm">添加</div>
                    </div>
                </div>
                <div class="second-chosen-firm"></div>
                <div class="edit-chosen-firm">
                    <div class="edit-chosen-firmName">
                        <span></span>
                        <input type="radio" name="" checked>
                    </div>
                    <div class="all-edit-chosen-employee"></div>
                    <div class="handle">
                       <!--  <div class="cancel-second-chosen">取消</div> -->
                        <div class="edit-chosen-added">添加</div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- 物料类别 -->
<script type="text/html" id="message-list-temp">
    {{each}}
        <div class="pr all-added-material" data-value={{$value.typeId}}>
            <div class="added-material">{{$value.typeName}}</div>
            <img src="images/del.png" class="delete-added-material">
        </div>
    {{/each}}
</script>

<script type="text/html" id="company-message-list">
    {{each}}
        <div class="company-message">
            <div class="firm">
                <span class="company-name">{{$value.name}}</span>
                <input type="radio" name="firmradio" class="firm-radio" data-firmId={{$value.id}}>
            </div>
            <div class="firm-employee">
                {{each $value.realtionUser}}
                <input type="checkbox"  data-userId={{$value.uId}} data-loginId={{$value.lId}}>
                <span class="employee-name">{{$value.uName}}</span>
                {{/each}}
            </div>
        </div>        
    {{/each}}
</script>
<script type="text/html" id="second-get-companyMessage">
    {{each}}
    <div class="secondsecond-chosen-firm">
        <div class="chosen-firmName">
            <div class="secondsecond-company-message">
                <div class="firm">
                    <span class="second-company-name" data-firmId={{$value.id}}>{{$value.name}}</span>
                </div>
                <img src="images/del.png" class="cancel-chosen-firm">
                <div class="second-firm-employee">
                    {{each $value.realtionUser}}
                        <span class="second-employee-name" data-userId={{$value.uId}} data-loginId={{$value.lId}}>{{$value.uName}}</span>
                    {{/each}}
                </div>
            </div> 
        </div>
    </div>               
    {{/each}}
</script>
