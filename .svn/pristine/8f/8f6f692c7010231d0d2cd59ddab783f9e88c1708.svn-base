<!-- 我的账户内容重写 -->
<div class="container-fulid container-myAccount-new" hidden="">
    <div class="myAccount-new cp" id="myAccount-newid">我的账户</div>   
    <div class="myAccount-new-contents">
        <div class="bcf myAccount-new-content">
            <!-- <div class="box-close" id="myAccountnewclose"></div> -->
            <nav class="nav-title tc disinblock myAccount-new-content">
                <ul class="disinblock color-8 f16">
                    <span class="color-6 pr disinblock myAccount-manage has nav-active"><li>个人资料</li></span>
                    <span class="color-6 pr disinblock myAccount-manage"><li>账户绑定</li></span>
                    <span class="color-6 pr disinblock myAccount-manage"><li>修改密码</li></span>
                </ul>
            </nav>
            <div class="personal-data" id="personal-data">
                <div class="myAccount-new-headerImg">
                    <img src="images/pic1.jpg" class="img-responsive img-responsive img-circle myAccountImgFault">
                    <div id="myAccount-new-img">
                        <div id="myAccountImgContainer" class="uploader-list"></div>
                        <div id="myAccountImgbtn">上传图像</div>
                    </div>
                </div>
                <div class="personal-data-content">
                    <form class="form-inline">
                        <div class="form-group">
                            <label for="nav-name" class="disem">昵称</label>
                            <input class="form-control" id="nav-name">
                        </div>
                        <div class="form-group">
                            <label for="nav-truename">真实姓名</label>
                            <input class="form-control" id="nav-truename">
                        </div>
                        <div class="form-group">
                            <!-- <label for="exampleInputName2" class="disem">性别</label>
                            <input class="form-control"> -->
                            <label class="disem">性别</label>
                            <label class="radio-inline">
                                <input type="radio" name="sex" value="2" id="sexBoy"> 男
                            </label>
                            <label class="radio-inline">
                                <input type="radio" name="sex" value="1" id="sexGirl"> 女
                            </label>
                        </div>
                    </form>
                    <form class="form-inline">
                        <div class="form-group">
                            <label for="nav-age" class="disem">年龄</label>
                            <input  class="form-control" id="nav-age">
                        </div>
                        <div class="form-group">
                            <label for="nav-birth">出生日期</label>
                            <input type="text" class="form-control" id="nav-birth" onfocus="WdatePicker({skin:'whyGreen',maxDate:'%y-%M-%d'})">
                        </div>
                        <div class="form-group">
                            <label for="certificate" class="disem1">证件号</label>
                            <input class="form-control" id="certificate">
                        </div>
                    </form>
                    <form class="form-inline">
                        <div class="form-group">
                            <label for="nav-tellphone">手机号码</label>
                            <input type="text" class="form-control" id="nav-tellphone" placeholder="可在账户绑定设置" readonly>
                        </div>
                        <div class="form-group">
                            <label for="nav-email">邮箱地址</label>
                            <input class="form-control" id="nav-email" placeholder="可在账户绑定设置" readonly>
                        </div>
                        <div class="form-group">
                            <label for="nav-address">常住地址</label>
                            <input class="form-control" id="nav-address">
                        </div>
                    </form>
                </div>
                <div class="personal-data-footer">
                    <div class="personal-data-save" id="nav-submit">保存</div>
                </div>
            </div>
            <div class="account-bind" id="account-bind">
                <div class="account-bind-content">
                    <form class="form-inline discenter pad30-0">
                        <div class="form-group">
                            <label>电话号码</label>
                            <div class="input-group">
                              <input type="text" class="form-control" id="bindphone">
                              <div class="input-group-addon cp btnTellBind" id="immediateBind"></div>
                            </div>
                        </div>
                    </form>
                    <form class="form-inline discenter mb-p20">
                        <div class="form-group">
                            <label class="disem">邮箱</label>
                            <div class="input-group">
                              <input type="text" class="form-control" id="bindEmail">
                              <div class="input-group-addon cp btnEmailBind" id="btnEmailBind"></div>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
            <div class="account-password" id="account-password" hidden>
                <div class="account-password-content">
                    <form class="form-inline discenter mt-20-0">
                        <div class="form-group">
                            <label for="oldPwd" class="disem4">原密码</label>
                            <input class="form-control" type="password" placeholder="请输入原密码" id="oldPwd">
                        </div>
                    </form>
                    <form class="form-inline discenter">
                        <div class="form-group">
                            <label for="newId" class="disem4">新密码</label>
                            <input class="form-control" id="newId" type="password" placeholder="请输入新密码">
                        </div>
                    </form>
                    <form class="form-inline discenter mt-20-0">
                        <div class="form-group">
                            <label for="reNewPwd">再次输入新密码</label>
                            <input class="form-control" id="reNewPwd" type="password" placeholder="请再次输入新密码" >
                        </div>
                    </form>
                </div>
                <div class="account-password-footer">
                    <div class="account-password-save" id="ModifyPwdSave">保存</div>
                </div>
            </div>
        </div>    
    </div>
</div>
