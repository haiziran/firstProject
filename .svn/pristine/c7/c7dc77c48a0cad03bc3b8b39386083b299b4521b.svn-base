.b5-center>ul>li>span:nth-child(4) {
    width: 13.2%;
}

.b5-center>ul>li>span:nth-child(5) {
    width: 20%;
}

.b5-center>ul>li>span:nth-child(6) {
    width: 17%;
}

.b5-center>.b5-tbody>li>span:nth-child(2) {
    width: 16%;
    padding-left: 1%;
}
.b5-center>.b5-tbody>li>span:nth-child(3){
	width:10%;
	padding-left: 0.5%;
}
.b5-center>.b5-tbody>li>span:nth-child(4){
	width:14%;
	padding-left: 1.2%;
}

.b5-center>.b5-tbody>li>span:nth-child(5) {
    width: 20%;
    padding-left: 1%;
}
.b5-center>.b5-tbody>li>span:nth-child(6){
	width:17%;
	padding-left: 1.3%;
}

.b5-center ul.table-header>li {
    padding: 15px 1%;
