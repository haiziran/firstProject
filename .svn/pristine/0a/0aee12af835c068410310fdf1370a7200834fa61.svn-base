.project-manage{
	background-color: rgb(249, 249, 249);
	min-height: 700px;
	height: auto;
}
.project{
	border: 1px solid #ccc;
	border-radius: 5px;
	text-align: center;
	padding: 10px;
	background-color: #fff;
}
.null-div{
	width: 100%;
	height: 25px;
}
.title-name{
	font-size: 18px;
	color: #333;
}
.project-name{
	text-align: left;
	align-items:center;
	display:-webkit-flex;
	height: 50px;
}
.detail{
	text-align: left;
}
